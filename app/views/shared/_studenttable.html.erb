<table>
  <tr>
    <th>Application Number</th>
    <th>Course</th>		
    <th>Status</th>
    <th>Last Updated</th>
  </tr>
  	<% @submissions.each do |sub |%>
		  <tr>
			    <td><%= sub.id %></td>
			    <td><%= sub.course.subject %></td>
			    <td><%= sub.status %></td>
			    <td><%= sub.updated_at.strftime("%F at %H:%M") %></td>
		  </tr>      
	   <% end %>
</table>
