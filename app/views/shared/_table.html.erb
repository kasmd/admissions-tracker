<table style="width:100%">
  <tr>
    <th>Application Number</th>
    <th>Course</th>		
    <th>Status</th>
    <th>Last Updated</th>
    <th>Details</th>
  </tr>
  	<% @submissions.each do |sub |%>
		  <tr>
			    <td><%= sub.id %></td>
			    <td><%= sub.course.subject %></td>
			    <td><%= sub.status %></td>
			    <td><%= sub.updated_at %></td>
			    <td><a href="submissions/<%=sub.id%>">Application Details</a></td>
		  </tr>      
	   <% end %>
</table>