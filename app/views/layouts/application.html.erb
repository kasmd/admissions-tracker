<!DOCTYPE html>
<html>
<head>
	<title>AdmissionsTracker</title>
	<%= stylesheet_link_tag    'application', media: 'all' %>
	<%= javascript_include_tag 'application' %>
	<%= csrf_meta_tags %>
</head>
<body>
<header>
	<%= render 'shared/header_logo' %>
	<nav class="float-right">
		<% if current_user %>
		<%= render 'shared/header_logged_in' %>
		<% else %>
		<%= render 'shared/header_public' %>
		<% end %>
	</nav>
</header>


	<div class="container">
  	<%= yield %>
	</div>


<%= render 'shared/footer' %>

</html>

</body>
</html>
